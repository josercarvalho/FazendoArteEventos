@model IEnumerable<Evento.Domain.Entity.Inscricao>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.FicouSabendo)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Socio)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Matricula)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Valor)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Periodo)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ValorPago)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Status)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Eventos)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Clientes)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.FicouSabendo)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Socio)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Matricula)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Valor)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Periodo)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ValorPago)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Status)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Eventos.FaixaEtaria)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Clientes.Bairro)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.InscricaoId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.InscricaoId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.InscricaoId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
